// Function definition
func = => "hola"

// Function call
func()

// Functions are objects
func.hola = 2

// Refer to function as `this`
my_func = a => {
  puts this.b
}

my_func() // # => undefined

my_func.b = 321

my_func() // # => 321

// Functions accept placeholder arguments
my_multiarg_func = (a, b, c) => a + b * c

// Returns a function equivalent to func = b => 2 + b * 4
my_multiarg_func(2, _, 4)
